    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.129",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/pouchdb": "^6.3.3",
    "@types/pouchdb-adapter-http": "^6.1.3",
    "@types/pouchdb-core": "^7.0.2",
    "@types/pouchdb-find": "^6.3.4",
    "@types/triple-beam": "^1.3.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint": "^5.16.0",
    "tslint-microsoft-contrib": "~6.1.1"
