    "feathers-pouchdb": "nickdex/feathers-pouchdb",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "mqtt": "^2.18.8",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-core": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "triple-beam": "^1.3.0",
    "typescript": "^3.4.5",
    "web-push": "^3.3.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
